
extends layout

block content
	div.container(style='margin:20px')
		table.table.table-bordered.table-striped
			thead
				tr
					th #
					th Name
					th Team
					th Username
					th mobile
					th email
					th Account Created
			tbody
			- for (var i = 0; i < accts.length; i++)
				tr
					td= i+1
					td= accts[i].name
					td= accts[i].team
					td= accts[i].user
					td= accts[i].mobile
					td= accts[i].email
					td= accts[i].date