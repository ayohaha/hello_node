
extends layout

block content
	div.container(style='margin:20px')
		table.table.table-bordered.table-striped
			thead
				tr
					th(style='width:40px') #
					th(style='width:100px') Name
					th(style='width:100px') Username
					th(style='width:200px') mobile
					th(style='width:180px') email
					th Account Created
			tbody
			- for (var i = 0; i < accts.length; i++)
				tr
					td= i+1
					td= accts[i].name
					td= accts[i].team
					td= accts[i].user
					td= accts[i].mobile
					td= accts[i].email
					td= accts[i].date